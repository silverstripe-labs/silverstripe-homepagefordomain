{
  "name": "twohill/silverstripe-homepagefordomain",
  "type": "silverstripe-vendormodule",
  "description": "Allows to designate a page anywhere in the tree as the homepage for a specific domain, overruling the usual homepage (which is identified by the 'home' URLSegment).",
  "keywords": [
    "silverstripe",
    "cms",
    "homepage",
    "domain",
    "subsites",
    "redirect"
  ],
  "license": "BSD-3-Clause",
  "homepage": "https://github.com/twohill/silverstripe-homepagefordomain",
  "replace": {
    "silverstripe/homepagefordomain": "*"
  },
  "authors": [
    {
      "name": "Al Twohill",
      "homepage": "https://al.twohill.nz",
      "email": "al@twohill.nz"
    }
  ],
  "autoload": {
    "psr-4": {
      "Twohill\\HomepageForDomain\\": "code/"
    }
  },
  "require": {
    "silverstripe/cms": "^4 || ^5"
  }
}
